{
  "name": "stimulus-password-visibility",
  "version": "2.1.1",
  "description": "A Stimulus controller to change a password input visibility.",
  "keywords": [
    "stimulus",
    "stimulusjs",
    "stimulus controller",
    "password toggle",
    "password visibility"
  ],
  "repository": "git@github.com:stimulus-components/stimulus-password-visibility.git",
  "bugs": {
    "url": "https://github.com/stimulus-components/stimulus-password-visibility/issues"
  },
  "author": "Guillaume Briday <guillaumebriday@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/stimulus-components/stimulus-password-visibility",
  "private": false,
  "main": "dist/stimulus-password-visibility.umd.js",
  "module": "dist/stimulus-password-visibility.mjs",
  "scripts": {
    "format": "prettier-standard  '**/*.{ts,css,html}' --format",
    "lint": "prettier-standard  '**/*.{ts,css,html}' --lint",
    "dev": "vite",
    "prod": "vite build --mode netlify",
    "build": "tsc --noEmit && vite build",
    "version": "yarn build",
    "test": "BABEL_ENV=test jest",
    "np": "np --no-2fa"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@hotwired/stimulus": "^3.2.1",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/dom": "^8.19.1",
    "@types/jest": "^29.2.4",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mutationobserver-shim": "^0.3.7",
    "np": "^7.6.2",
    "postcss": "^8.4.20",
    "prettier-standard": "16.4.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.3"
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.0.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/jest/jest-setup.js"
    ]
  }
}
